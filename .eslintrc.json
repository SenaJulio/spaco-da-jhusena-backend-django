{
  "env": {
    "browser": true,
    "es2022": true
  },
  "globals": {
    "Chart": "readonly",
    "bootstrap": "readonly"
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [
      "warn",
      { "args": "none", "ignoreRestSiblings": true }
    ],
    "no-undef": "error",
    "import/order": [
      "warn",
      { "newlines-between": "always" }
    ],
    "no-console": "off"
  }
}
